	"version": "0.2.0",
	"configurations": [
		
		{
			"name": "(lldb) Launch",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/test",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"name": "(gdb) Запустить",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/1234/minishell",
			"args": [],
			"stopAtEntry": true,
			"cwd": "${workspaceFolder}/",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Включить автоматическое форматирование для gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			]
		}
	]
}